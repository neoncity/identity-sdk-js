{
  "name": "@neoncity/identity-sdk-js",
  "version": "0.0.1",
  "description": "The SDK for the identity service.",
  "author": "NeonCity team",
  "scripts": {
    "build": "tsc",
    "pretest": "tsc",
    "test-dev": "tsc --watch & mocha --watch $(find out -name '*-test.js')",
    "test": "tsc && istanbul cover --config ./.istanbul.yml ./node_modules/mocha/bin/_mocha --  $(find out -name '*-test.js') && remap-istanbul -i ./build/coverage/coverage-final.json -o ./build/coverage/coverage-remapped.json && coverage-filter -i ./build/coverage/coverage-remapped.json -o ./build/coverage/coverage-filtered.json",
    "push-coverage-to-codecov": "codecov -f ./build/coverage/coverage-filtered.json"
  },
  "keywords": [
    "identity",
    "sdk"
  ],
  "respository": {
    "type": "git",
    "url": "git+https://github.com/neoncity/identity-sdk-js.git"
  },
  "private": true,
  "homepage": "https://github.com/neoncity/identity-sdk-js#readme",
  "main": "./index.js",
  "types": "./index.d.ts",
  "filesPack": {
    "out": "e:.",
    "README.md": "f:README.md",
    "package.json": "f:package.json",
    "tsconfig.json": "f:tsconfig.json"
  },
  "license": "UNLICENSED",
  "dependencies": {},
  "devDependencies": {
    "@neoncity/common-js": "^0.12.1",
    "@neoncity/coverage-filter": "^0.0.7",
    "@neoncity/togemfury": "^0.0.9",
    "@types/chai": "^3.4.34",
    "@types/istanbul": "^0.4.29",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "remap-istanbul": "^0.8.4",
    "typescript": "^2.1.5"
  }
}
